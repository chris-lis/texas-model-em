{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "import-name": false,
    "interface-name": false,
    "variable-name": false,
    "object-literal-sort-keys": false,
  },
  "linterOptions": {
    "exclude": ["node_modules/**/*", ".next/**/*"]
  }
}
